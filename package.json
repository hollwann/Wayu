    "license": "MIT",
    "bugs": {
        "url": "https://github.com/hollwann/Wayu/issues"
    },
    "homepage": "https://github.com/hollwann/Wayu#readme",
    "dependencies": {
        "@types/mysql": "^2.15.21",
        "@typescript-eslint/parser": "^5.30.6",
        "db-migrate": "^0.11.13",
        "db-migrate-mysql": "^2.2.0",
        "mysql": "^2.18.1",
        "nodemon": "^1.19.4",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@types/node": "^18.0.0",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.29.0",
        "eslint": "^8.18.0",
        "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
