        "lint": "prettier --write \"./src/**/*.ts\" && eslint --fix --ext .ts src",
        "lint:check": "prettier --check \"./src/**/*.ts\" && eslint --ext .ts src"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hollwann/Wayu.git"
    },
    "keywords": [
        "node",
